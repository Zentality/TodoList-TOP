{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "max-len": "off",
      "quotes": [
        "error",
        "double"
      ],
      "no-use-before-define": "off"
    }
}
